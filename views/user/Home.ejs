<!-- ================= SLIDER START ================= -->
<div id="mainCarousel" class="carousel slide container" data-bs-ride="carousel">
  <div class="carousel-inner">
    <% sliders.forEach((i, index) => { %>
      <div class="carousel-item <%= index === 0 ? 'active' : '' %>">
        <img src="/upload/<%= i.Slider_img %>" class="d-block w-100" alt="Banner">
        <!-- Text Over Image -->
        <div class="carousel-caption d-none d-md-block text-start">
          <h1 class="fw-bold"><%= i.Slider_title %></h1>
          <p class="fs-4"><%= i.Slider_heading %></p>
        </div>
      </div>
    <% }) %>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#mainCarousel" data-bs-slide="prev">
    <span class="carousel-control-prev-icon"></span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#mainCarousel" data-bs-slide="next">
    <span class="carousel-control-next-icon"></span>
  </button>
</div>
<!-- ================= SLIDER END ================= -->





















<!-- ================= TRACTOR DECORATION PRODUCTS START ================= -->
<div class="container my-5">
  
  <h4 class="text-center mb-4">Tractor Decoration Accessories</h4>

  <!-- ROW should be outside loop -->
  <div class="row justify-content-center g-4">

    <% tractorDecorations.forEach((i, index) => { %>

      <div class="col-6 col-sm-4 col-md-2">
        <div class="product-card text-center">
          <img src="/upload/<%= i.tractor_dec_img %>" 
               alt="<%= i.tractor_dec_title %>" 
               class="img-fluid">
        <h6 class="tractor-title">
  <a href="/all_tractor_dec"><%= i.tractor_dec_title %></a>
</h6>

          <p>₹ <%= i.tractor_dec_price %></p>
        </div>
      </div>

    <% }) %>

  </div>
</div>


<!-- ================= TRACTOR DECORATION PRODUCTS END ================= -->
<!-- ================= VEHICLE DECORATION PRODUCTS ================= -->
<div class="container mt-4">

    <% vehicleDecorations.forEach((i) => { %>

        <h4 class="text-center mb-4"><%= i.vehicle_dec_title %></h4>

        <div class="custom-banner mb-5">

            <!-- LEFT -->
            <div class="banner-left">
                <img src="/upload/<%= i.vehicle_dec_img %>" 
                     alt="<%= i.vehicle_dec_title %>">
            </div>

            <!-- RIGHT -->
            <div class="banner-right">
                <h4><%= i.vehicle_dec_heading %></h4>
                <p><%= i.vehicle_dec_desc %></p>

                <a href="/vehicle/<%= i.vehicle_dec_id %>" 
                   class="btn-explore text-decoration-none">
                    Explore Now →
                </a>
            </div>

        </div>

    <% }) %>

</div>
