<!-- SLIDER SCRIPT -->
<script>
  let currentSlide = 0;
  const slides = document.querySelectorAll(".home-slider .slide");

  if (slides.length) {
    setInterval(() => {
      slides[currentSlide].classList.remove("active");
      currentSlide = (currentSlide + 1) % slides.length;
      slides[currentSlide].classList.add("active");
    }, 4000);
  }
</script>



