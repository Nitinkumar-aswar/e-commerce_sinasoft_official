    /* =========================
       RESPONSIVE MEDIA QUERIES
    ========================= */
    @media (max-width: 1200px) {
      .home-slider { width: 100%; margin: 20px auto; }
      .shop-banner-left { width: 100%; margin: 0 0 14px 0; border-right: none; padding-right: 0; }
      .shop-products-right { width: 100%; }
      .vehicle-left { width: 100%; margin-bottom: 10px; }
      .vehicle-right { width: 100%; }
      .meesho-categories { grid-template-columns: repeat(3, 1fr); }
      .product-grid { grid-template-columns: repeat(3, 1fr); }
      .shop-product-row { grid-template-columns: repeat(2, 1fr); }
    }

    @media (max-width: 768px) {
      .home-slider { height: 220px; }
      .vehicle-wrapper { flex-direction: column; }
      .vehicle-left, .vehicle-right { width: 100%; }
      .shop-wrapper { flex-direction: column; }
      .shop-banner-left, .shop-products-right { width: 100%; margin: 0; padding: 0; border: none; }
      .meesho-categories { grid-template-columns: repeat(2, 1fr); gap: 10px; }
      .product-grid { grid-template-columns: repeat(2, 1fr); }
      .shop-product-row { grid-template-columns: 1fr; }
    }

    @media (max-width: 480px) {
      .home-slider { height: 180px; }
      .meesho-categories { grid-template-columns: 1fr; gap: 10px; }
      .product-grid { grid-template-columns: 1fr; }
      .footer-dark .row > [class*="col-"] { text-align: center !important; }
    }



    /* ================= MOBILE RESPONSIVE FOOTER ================= */
@media (max-width: 768px) {
  .footer-dark .row {
    flex-direction: column;
    align-items: center;
