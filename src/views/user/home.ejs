<script>
  // =========================
  // FLIPKART TOP SLIDER SCRIPT
  // =========================

  const fkSlides = document.querySelectorAll(".fk-slide");
  const fkDots = document.querySelectorAll(".fk-dots .dot");
  const fkNext = document.querySelector(".fk-next");
  const fkPrev = document.querySelector(".fk-prev");

  let fkCurrent = 0;

  function showFkSlide(index) {
    fkSlides.forEach((slide, i) => {
      slide.classList.toggle("active", i === index);
      fkDots[i].classList.toggle("active", i === index);
    });
    fkCurrent = index;
  }

  // AUTO SLIDE (3 sec)
  setInterval(() => {
    showFkSlide((fkCurrent + 1) % fkSlides.length);
  }, 3000);

  // MANUAL CONTROLS
  fkNext.onclick = () =>
    showFkSlide((fkCurrent + 1) % fkSlides.length);

  fkPrev.onclick = () =>
    showFkSlide((fkCurrent - 1 + fkSlides.length) % fkSlides.length);

  fkDots.forEach((dot, i) => {
    dot.onclick = () => showFkSlide(i);
  });
</script>

 
